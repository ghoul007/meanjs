{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "forever -e ./log/error.log ./bin/www",
    "dev": " nodemon  ./bin/www",
    "stop": "forever stopall",
    "log": "DEBUG_COLORS=no DEBUG=* nodemon ./bin/www",
    "debug": "node --inspect --debug-brk  ./bin/www",
    "security": "auditjs",
    "test": "tap ./routes/api/**/*spec.js",
    "cover": "tap ./test/*.js --cov --coverage-report=lcov"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "body-parser": "~1.18.2",
    "class.extend": "^0.9.2",
    "cookie-parser": "~1.4.3",
    "debug": "~3.1.0",
    "dotenv": "^4.0.0",
    "express": "~4.16.2",
    "express-bearer-token": "^2.1.0",
    "express-mung": "^0.4.6",
    "express-session": "^1.15.6",
    "forever": "^0.15.3",
    "helmet": "^3.9.0",
    "http": "^0.0.0",
    "https": "^1.0.0",
    "jsonapi-serializer": "^3.5.5",
    "jwt-express": "^1.1.0",
    "lodash": "^4.17.4",
    "morgan": "~1.9.0",
    "nodemon": "^1.12.1",
    "object.values": "^1.0.4",
    "resource-router-middleware": "^0.7.0"
  },
  "devDependencies": {
    "casual": "^1.5.19",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.1",
    "supertest": "^3.0.0",
    "tap-only": "0.0.5"
  }
}
